    "@nuxtjs/eslint-module": "4.1.0",
    "@nuxtjs/google-fonts": "3.0.2",
    "@types/node": "18.17.3",
    "@unocss/nuxt": "0.55.4",
    "@unocss/transformer-compile-class": "0.55.4",
    "@unocss/transformer-directives": "^0.55.4",
    "@unocss/transformer-variant-group": "0.55.4",
    "@vueuse/core": "10.4.0",
    "@vueuse/nuxt": "10.4.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
